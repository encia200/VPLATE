<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.vplate.activity.PwSetCompleteActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="25sp"
        android:textColor="#1d1d1d"
        android:text="비밀번호가
재설정되었습니다!"
        android:layout_marginTop="56dp"
        android:layout_marginLeft="32dp"/>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="11.5sp"
        android:textColor="#4c4d4e"
        android:text="단 몇 분만에 고품질의 영상을 만들어보세요"
        android:layout_marginTop="9dp"
        android:layout_marginLeft="32dp"/>

    <ImageButton
        android:id="@+id/pw_set_complete_loginBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/letslogin"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="78.5dp"/>


</LinearLayout>
